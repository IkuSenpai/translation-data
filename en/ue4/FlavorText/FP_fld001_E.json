[
    {
        "id": "100",
        "id_number": 100,
        "value": "お手製のかかしが立てられている",
        "translation": ""
    },
    {
        "id": "101",
        "id_number": 101,
        "value": "なかなか味のある面構えだ",
        "translation": ""
    },
    {
        "id": "102",
        "id_number": 102,
        "value": "胴体から網が発射されて動物を捕まえるらしい。\nうかつにさわるのはやめておこう",
        "translation": ""
    },
    {
        "id": "200",
        "id_number": 200,
        "value": "村の薪置き場だ",
        "translation": ""
    },
    {
        "id": "201",
        "id_number": 201,
        "value": "お湯を沸かしたり、パン焼きの窯に火を入れたり\nするときには薪が必要になる",
        "translation": ""
    },
    {
        "id": "202",
        "id_number": 202,
        "value": "バファリアの遺産は便利だが\n生活のすべてがそれで賄われるわけではないようだ",
        "translation": ""
    },
    {
        "id": "203",
        "id_number": 203,
        "value": "薪割りも大切な仕事のひとつだ",
        "translation": ""
    },
    {
        "id": "300",
        "id_number": 300,
        "value": "ミンスターホルンで作られた野菜や果物、ミルク、\n色とりどりの花などが売られている",
        "translation": ""
    },
    {
        "id": "301",
        "id_number": 301,
        "value": "陶器や革袋、小さな家具といった\n手作りの工芸品もあるようだ",
        "translation": ""
    },
    {
        "id": "302",
        "id_number": 302,
        "value": "値段もお手ごろだ。\nおみやげにちょうどいいかもしれない",
        "translation": ""
    },
    {
        "id": "400",
        "id_number": 400,
        "value": "歴代のミンスターホルンの村長の名前と\nそれぞれの功績が刻まれている",
        "translation": ""
    },
    {
        "id": "401",
        "id_number": 401,
        "value": "『冒険者ターベッティと４人の仲間が森を拓き\n風車を建てたことがこの地の始まりである』",
        "translation": ""
    },
    {
        "id": "402",
        "id_number": 402,
        "value": "『セブラン三等神官は生涯をささげて\nバファリアの教えを人々に伝えた』",
        "translation": ""
    },
    {
        "id": "403",
        "id_number": 403,
        "value": "『なり手がいなかったので野良猫のリシャールを\n村長にしたところ、訪れる人が急激に増えた』",
        "translation": ""
    },
    {
        "id": "500",
        "id_number": 500,
        "value": "建物の塔屋には古い風車がゆっくりと回っている",
        "translation": ""
    },
    {
        "id": "501",
        "id_number": 501,
        "value": "今も粉ひきや水汲みに活用されているようだ",
        "translation": ""
    },
    {
        "id": "502",
        "id_number": 502,
        "value": "削除",
        "translation": ""
    },
    {
        "id": "600",
        "id_number": 600,
        "value": "『よくぞこんな場所に気がついた。\n褒美にちょいとした知識を授けてしんぜよう』",
        "translation": ""
    },
    {
        "id": "601",
        "id_number": 601,
        "value": "『ゴブリンやムーク、オーガといった亜人は\n集落を作って集団生活をしておる』",
        "translation": ""
    },
    {
        "id": "602",
        "id_number": 602,
        "value": "『よく探せばあやつらの根城が見つかるやもしれん。\n変わった道具や料理があるやもしれんぞ",
        "translation": ""
    },
    {
        "id": "603",
        "id_number": 603,
        "value": "『あちこちくまなくめぐってみなされ。\nほれ、ここにもいい話があったじゃろ？　ほっほっほ』",
        "translation": ""
    },
    {
        "id": "700",
        "id_number": 700,
        "value": "かなり古びた墓標だ。\n折れた剣がひっそりと供えられている",
        "translation": ""
    },
    {
        "id": "701",
        "id_number": 701,
        "value": "『竜殺しの英雄リオフィール　ここに眠る』",
        "translation": ""
    },
    {
        "id": "702",
        "id_number": 702,
        "value": "かつてアステルリーズに押し寄せた竜族を\n死闘の末に退けた冒険者の名前だ",
        "translation": ""
    },
    {
        "id": "703",
        "id_number": 703,
        "value": "竜王の腹心とされた竜族を相手に一歩も引かず\n最期まで街と人々を守り通したという",
        "translation": ""
    },
    {
        "id": "704",
        "id_number": 704,
        "value": "戦いを終えた英雄は\n人知れず静かな眠りについているようだ",
        "translation": ""
    },
    {
        "id": "800",
        "id_number": 800,
        "value": "風に乗って獣の声が聞こえてくる",
        "translation": ""
    },
    {
        "id": "801",
        "id_number": 801,
        "value": "空の彼方を飛ぶ影がある。\n鳥か、それとも竜だろうか",
        "translation": ""
    },
    {
        "id": "802",
        "id_number": 802,
        "value": "ときには頭上に注意を向けることも\n冒険者の心得だ",
        "translation": ""
    },
    {
        "id": "900",
        "id_number": 900,
        "value": "破壊された機械の残骸が転がっている",
        "translation": ""
    },
    {
        "id": "901",
        "id_number": 901,
        "value": "中の部品がいくつもなくなっている。\n誰かが持ち去った後のようだ",
        "translation": ""
    },
    {
        "id": "902",
        "id_number": 902,
        "value": "……こじ開けられた外板に体毛が引っかかっている。\nゴブリンのものだ",
        "translation": ""
    },
    {
        "id": "1000",
        "id_number": 1000,
        "value": "バファリア遺跡の残骸だ",
        "translation": ""
    },
    {
        "id": "1001",
        "id_number": 1001,
        "value": "大きな穴が空いてひしゃげている。\nなにか太く、鋭いもので貫かれたかのようだ",
        "translation": ""
    },
    {
